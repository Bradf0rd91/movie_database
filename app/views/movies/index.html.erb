<table class="table table-striped table-bordered">
  <tr>
    <th>Title</th>
    <th>Year</th>
    <th>Format</th>
    <th>Loanee</th>
  </tr>
  <% @movies.each do |m| %>
    <tr>
      <td><%= m.title %></td>
      <td><%= m.year %></td>
      <td><%= m.rec_form %></td>
      <td><%= m.loanee %></td>
    </tr>
  <% end %>
</table>